{
  "version": 1,
  "projects": {
    "sb": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/sb",
      "sourceRoot": "apps/sb/src",
      "prefix": "experiment-monorepo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/sb",
            "index": "apps/sb/src/index.html",
            "main": "apps/sb/src/main.ts",
            "polyfills": "apps/sb/src/polyfills.ts",
            "tsConfig": "apps/sb/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/sb/src/favicon.ico", "apps/sb/src/assets"],
            "styles": ["apps/sb/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sb/src/environments/environment.ts",
                  "with": "apps/sb/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sb:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sb:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sb:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/sb/tsconfig.app.json",
              "apps/sb/tsconfig.spec.json",
              "apps/sb/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/sb/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/sb/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sb-e2e": {
      "root": "apps/sb-e2e",
      "sourceRoot": "apps/sb-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sb-e2e/cypress.json",
            "tsConfig": "apps/sb-e2e/tsconfig.e2e.json",
            "devServerTarget": "sb:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sb:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/sb-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/sb-e2e/**/*"]
          }
        }
      }
    },
    "lgt-date": {
      "projectType": "library",
      "root": "libs/lgt-date",
      "sourceRoot": "libs/lgt-date/src",
      "prefix": "experiment-monorepo",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/lgt-date/tsconfig.lib.json",
              "libs/lgt-date/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/lgt-date/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/lgt-date/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "smartbanking-showcase": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/smartbanking/showcase",
      "sourceRoot": "apps/smartbanking/showcase/src",
      "prefix": "experiment-monorepo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/smartbanking/showcase",
            "index": "apps/smartbanking/showcase/src/index.html",
            "main": "apps/smartbanking/showcase/src/main.ts",
            "polyfills": "apps/smartbanking/showcase/src/polyfills.ts",
            "tsConfig": "apps/smartbanking/showcase/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/smartbanking/showcase/src/favicon.ico",
              "apps/smartbanking/showcase/src/assets"
            ],
            "styles": ["apps/smartbanking/showcase/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/smartbanking/showcase/src/environments/environment.ts",
                  "with": "apps/smartbanking/showcase/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "smartbanking-showcase:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "smartbanking-showcase:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "smartbanking-showcase:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/smartbanking/showcase/tsconfig.app.json",
              "apps/smartbanking/showcase/tsconfig.spec.json",
              "apps/smartbanking/showcase/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/smartbanking/showcase/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/smartbanking/showcase/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "smartbanking-showcase-e2e": {
      "root": "apps/smartbanking/showcase-e2e",
      "sourceRoot": "apps/smartbanking/showcase-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/smartbanking/showcase-e2e/cypress.json",
            "tsConfig": "apps/smartbanking/showcase-e2e/tsconfig.e2e.json",
            "devServerTarget": "smartbanking-showcase:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "smartbanking-showcase:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/smartbanking/showcase-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/smartbanking/showcase-e2e/**/*"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "sb"
}
